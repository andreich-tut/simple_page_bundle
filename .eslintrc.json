{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-tabs": 0,
        "no-console": 0,
        "new-cap": 0,
        "no-trailing-spaces": 0,
        "no-multi-spaces": 0,
        "linebreak-style": 0,
        "eol-last": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "indent": [
            0,
            "spaces",
            4
        ]
    }
}